using System;

namespace test
{
    class MainClass
    {

        public static void Main(string[] args)
        {


            Console.WriteLine("Welcome To Loop");

            for (int i = 0; i < 3; i++)
            {
                Console.WriteLine("i am I  : " + i);

                for(int j = 0; j<5; j++)
                {
                    Console.WriteLine("j : " + j);
                }
            }

          





        }

    }
}
